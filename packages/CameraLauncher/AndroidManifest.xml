<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="org.teameos.apps.cameralauncher"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <application android:label="@string/camera_label" android:icon="@drawable/ic_launcher_camera">
        <activity android:label="@string/camera_label" android:name="org.teameos.apps.cameralauncher.CameraLauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>
